			<p>{{_nitro.pageTitle}} = pageTitle (view)</p>
			<p>{{test.text}} = test.text (view)</p>

			{{#unless _nitro.production}}
				{{pattern 't' 't-red'}}
				<hr>
				{{pattern 't'}}
				<hr>
				{{pattern 't' 't-red'}}
