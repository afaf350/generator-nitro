<div class="m-example{% if modifier %} m-example--{{modifier}}{% endif %}" data-t-name="Example"{% if decorator %} data-t-decorator="{{decorator}}"{% endif %}>
	<h2 class="m-example__headline">{{title}}</h2>
	<ul class="js-m-example__list">
		{% for link in links %}
			<li><button class="m-example__button">{{link.text}}</button></li>
		{% endfor %}
	</ul>
</div>
