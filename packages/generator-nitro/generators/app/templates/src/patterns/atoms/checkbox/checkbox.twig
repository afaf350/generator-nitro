<{{tag}} class="a-checkbox{% if _state %} state-a-checkbox--{{_state}}{% endif %}">
<input type="checkbox" id="{{inputId}}" name="{{inputName}}" value="{{inputValue}}"{% if checked %} checked{% endif %}>
{% if label %}<label class="a-checkbox__label" for="{{inputId}}">{{label}}</label>{% endif %}
</{{tag}}>
