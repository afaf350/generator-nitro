{
  "name": "generator-nitro",
  "version": "4.0.0-rc.1",
  "description": "Yeoman generator for the nitro frontend framework",
  "license": "MIT",
  "repository": "namics/generator-nitro",
  "author": "The Nitro Team",
  "engines": {
    "node": ">=6 <11",
    "npm": ">=3 <7"
  },
  "main": "generators/app/index.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "jasmine --config=tests/jasmine/support/jasmine.json",
    "prepublish": "nsp check --output summary",
    "prepublishOnly": "pkg-ok --bin generators/app/templates/project/.githooks/pre-commit generators/app/templates/project/.githooks/post-merge"
  },
  "files": [
    "generators/app",
    "generators/pattern",
    "generators/component"
  ],
  "keywords": [
    "yeoman-generator",
    "scaffold",
    "frontend",
    "app",
    "nitro",
    "component",
    "pattern",
    "modular"
  ],
  "dependencies": {
    "adm-zip": "0.4.7",
    "chalk": "2.4.1",
    "config": "1.30.0",
    "git-config": "0.0.7",
    "glob": "7.1.2",
    "lodash": "4.17.10",
    "request": "2.87.0",
    "yeoman-generator": "3.0.0",
    "yosay": "2.0.2"
  },
  "devDependencies": {
    "@namics/eslint-config": "5.0.1",
    "ejs": "2.6.1",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.13.0",
    "fs-extra": "7.0.0",
    "jasmine": "3.1.0",
    "nsp": "3.2.1",
    "pkg-ok": "2.2.0",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.8.0"
  }
}
